<% include partials/header.ejs %>

<div class="container">
  <div class="row">
    <div class="page-title">
      <h1>Admin Login</h1>
    </div>
    <div class="row">
      <form id="admin-login-form" class="col span-full">
        <div class="row">
          <div class="span-1-of-6">Username</div>
          <input type="textbox" class="span-1-of-3" required />
        </div>
        <div class="row">
          <div class="span-1-of-6">Password</div>
          <input type="password" class="span-1-of-3" required />
        </div>
        <div class="row">
          <div class="span-full">
            <button type="submit" class="btn btn-light">
              Log In
            </button>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>

<% include partials/footer.ejs %>
